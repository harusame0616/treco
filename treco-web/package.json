{
  "name": "treco-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='-r newrelic' next dev",
    "dev:no-auth-emulator": "next dev",
    "prebuild": "npm run format:check && prisma generate && prisma migrate deploy",
    "build": "next build",
    "start": "GOOGLE_CLIENT_ID=id GOOGLE_CLIENT_SECRET=secret NEXT_AUTH_SECRET=secret NODE_OPTIONS='-r @newrelic/next' next start",
    "start:auth-jwt-no-encryption": "NEXT_AUTH_JWT_NO_ENCRYPTION=true GOOGLE_CLIENT_ID=id GOOGLE_CLIENT_SECRET=secret NEXT_AUTH_SECRET=secret NODE_OPTIONS='-r @newrelic/next' next start",
    "lint:check": "next lint",
    "lint:write": "next lint --fix",
    "format:check": "prettier './**/*.{ts,tsx,json}' --check",
    "format:write": "prettier './**/*.{ts,tsx,json}' --write",
    "db:seed": "prisma db seed",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@harusame0616/pino-newrelic-log-api-transport": "^0.1.0",
    "@hookform/resolvers": "^3.3.2",
    "@newrelic/native-metrics": "^10.0.1",
    "@newrelic/next": "^0.7.0",
    "@prisma/client": "^5.4.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@types/react-slick": "^0.23.10",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.10",
    "firebase": "^10.4.0",
    "firebase-admin": "^11.11.0",
    "newrelic": "^11.5.0",
    "next": "latest",
    "next-auth": "^4.23.2",
    "pino": "^8.16.1",
    "prisma": "^5.4.2",
    "react": "latest",
    "react-dom": "latest",
    "react-hook-form": "^7.47.0",
    "react-slick": "^0.29.0",
    "slick-carousel": "^1.8.1",
    "swr": "^2.2.4",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7",
    "valibot": "^0.18.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.39.0",
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "autoprefixer": "latest",
    "eslint": "latest",
    "eslint-config-next": "latest",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-perfectionist": "^2.2.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "postcss": "latest",
    "prettier": "^3.0.3",
    "tailwindcss": "latest",
    "ts-node": "^10.9.1",
    "typescript": "latest"
  }
}
